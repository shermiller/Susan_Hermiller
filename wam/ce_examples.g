


# Finding the order of a group, the
#index of a subgroup, and the coset table information:


F2 := FreeGroup( "a", "b" );

G := F2 / [ F2.1^2, F2.2^2, (F2.1*F2.2)^2 ];
IsFinite(G);
Size(G);
a := G.1;; b := G.2;;

H := Subgroup( G, [ a*b ] );
Index( G, H );
tabh := CosetTable(G, H);;
PrintArray( TransposedMat( tabh ) );

J  := Subgroup( F2, [ F2.1, F2.2*F2.1*F2.2^-1, 
  F2.2^-1*F2.1*F2.2, F2.2^2 ] );;
Index( F2, J );
tabj := CosetTable(F2, J);
PrintArray( TransposedMat( tabj ) );

K := Subgroup( F2, [ (F2.1*F2.2)^2, (F2.1^-1*F2.2)^2 ] );;
Index( F2, K );
tabk := CosetTable(F2, K);

quit;


