# PSL_2(Z)

F2 := FreeGroup( "a", "b" );

G := F2 / [ F2.1^2, F2.2^3 ];
a := G.1;; b := G.2;;

H := Subgroup( G, [ b*a*b*a^-1*b, b^-1*a*b*a^-1 ] );
Index( G, H );
tabh := CosetTable(G, H);;
PrintArray( TransposedMat( tabh ) );


p := PresentationSubgroup( G, H, "G" );
gens := GeneratorsOfPresentation( p );
TzPrintRelators( p );

