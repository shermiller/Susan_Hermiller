#Groups generated by reflections 


F3 := FreeGroup( "a", "b", "c" );
C := F3 / [ F3.1^2, F3.2^2, F3.3^2, (F3.1*F3.2)^3, 
    (F3.1*F3.3)^3, (F3.2*F3.3)^3 ];
### IsFinite(C);
a := C.1;; b := C.2;; c:= C.3;;

N := Subgroup( C, [ a*b*a*c, a*c*a*b ] );
Index( C, N );
tabn := CosetTable(C, N);;
PrintArray( TransposedMat( tabn ) );



F3 := FreeGroup( "a", "b", "c" );
D := F3 / [ F3.1^2, F3.2^2, F3.3^2, (F3.1*F3.2)^8, 
    (F3.1*F3.3)^8, (F3.2*F3.3)^8 ];
a := D.1;; b := D.2;; c:= D.3;;

M := Subgroup( D, [ a, b, c*a*c, c*b*c ] );
Index( D, M );
tabn := CosetTable(D, M);;
PrintArray( TransposedMat( tabn ) );


